<odoo>
  <data>

    <record id="mis_inv_adjustment_inherit" model="ir.ui.view">
        <field name="name">mis.inv.adjustment.inherit</field>
        <field name="model">stock.inventory</field>
        <field name="inherit_id" ref="stock.view_inventory_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_ids']" position="after">
                <field name="aac_id" attrs="{'domain': [('company_id','=','company_id')]}"></field>
            </xpath>
        </field>
    </record>

  </data>
</odoo>