<odoo>
    
    <record id="group_list_detail_penerimaan" model="res.groups">
        <field name="name">List Detail Penerimaan Report</field>
    </record>

    <record model="ir.rule" id="rule_report_detail_penerimaan_global_user">
        <field name="name">Rule Detail Penerimaan List Global User</field>
        <field name="model_id" ref="model_list_detail_penerimaan"/>
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="False" />
        <field name="perm_write" eval="False" />
        <field name="perm_unlink" eval="False" />
        <field name="domain_force">['|', ('company_id','=',False),('company_id','in',company_ids)]</field>
    </record>
</odoo>

