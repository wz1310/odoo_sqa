<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Sequences for customer code -->
        <record id="seq_customer_code" model="ir.sequence">
            <field name="name">Customer Code</field>
            <field name="code">seq.customer.empty</field>
            <field name="prefix">WIM-</field>
            <field name="padding">4</field>
            <field name="company_id"></field>
        </record>

        <record id="seq_contact_change_request" model="ir.sequence">
            <field name="name">Contact Change Request Seq</field>
            <field name="code">seq.contact.change.request</field>
            <field name="prefix">EDITCUST/%(day)s/%(month)s/%(year)s-</field>
            <field name="use_date_range">True</field>
            <field name="padding">4</field>
            <field name="company_id"></field>
        </record>
    </data>
</odoo>
