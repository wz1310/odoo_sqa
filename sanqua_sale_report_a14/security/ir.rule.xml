<odoo>
    
    <record id="group_credit_note_report" model="res.groups">
        <field name="name">A14 - Laporan Credit Note</field>
    </record>

    <record model="ir.rule" id="rule_report_credit_note_global_user">
        <field name="name">Rule Credit Note User</field>
        <field name="model_id" ref="model_report_credit_note" />
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="False" />
        <field name="perm_write" eval="False" />
        <field name="perm_unlink" eval="False" />
        <field name="domain_force">['|', ('company_id','=',False),('company_id','in',company_ids)]</field>
    </record>
</odoo>