<odoo>
    <record model="ir.ui.view" id="inherit_sale_agreement_form_view">
        <field name="name">inherit_sale_agreement_form_view</field>
        <field name="model">sale.agreement</field>
        <field name="inherit_id" ref="sale_agreement.sale_agreement_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='team_id']" position="attributes">
                <attribute name="domain">[('state','=','done')]</attribute>
            </xpath>
        </field>
    </record>
</odoo>