<odoo>
    <record model="ir.ui.view" id="view_stock_move_line_operation_tree">
        <field name="name">view_stock_move_line_operation_tree</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree" />
        <field name="arch" type="xml">
            <field name="lot_id" position="before">
                <field name="available_lot_in_location" invisible="1" />
            </field>
            <xpath expr="//field[@name='lot_id']" position="attributes">
                <attribute name="domain">[('id', 'in', available_lot_in_location)]</attribute>              
            </xpath>
            <field name="lot_id" position="after">
                <field name="free_qty" force_save="1" readonly="1"/>
            </field>
        </field>
    </record>
</odoo>