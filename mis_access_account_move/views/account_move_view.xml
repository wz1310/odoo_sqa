<odoo>
    <record id="mis_view_access_move_form_inherit" model="ir.ui.view">
        <field name="name">mis.access.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="cek_user" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="attributes">
                <attribute name="attrs">{'readonly': [('parent.cek_user', '=', False)]}</attribute>
            </xpath>
        </field>
    </record>
        
    <record id="group_acess_qty_pfi" model="res.groups">
        <field name="name">Access Qty PFI</field>
    </record>
    
</odoo>