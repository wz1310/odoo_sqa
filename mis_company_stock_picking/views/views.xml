<odoo>
  <data>
    <record model="ir.ui.view" id="in_inherited_view_stock_picking_form">
    <field name="name">Inherited View Stock Picking Form</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form" />
    <field name="arch" type="xml">

      <!-- replace origin sales_team_id position -->
      <!-- because we dont know if sales team used in other xpath expression or other so we only readonly and invisible it -->
      <xpath expr="//field[@name='name']" position="after">
        <field name="cek_akses" invisible="1"/>
      </xpath>
    </field>
  </record>
  </data>
</odoo>