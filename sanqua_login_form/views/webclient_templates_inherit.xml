<odoo>
    <template id="login_layout_inherit" name="sanqua_login_layout_inherit" inherit_id="web.login_layout">
        <xpath expr="//t[@t-call='web.frontend_layout']/div/div/div/div[1]/img" position="replace">
            <img t-attf-src="/sanqua_login_form/static/src/img/sanqua-logo.png" alt="Logo" style="max-height:120px; max-width: 100%; width:auto"/>
        </xpath>
    </template>
</odoo>