<odoo>
        <record id="view_create_pbbh" model="ir.ui.view">
            <field name="name">Create PBBH</field>
            <field name="model">wizard.create.pbbh</field>
            <field name="arch" type="xml">
                <form string="Create PBBH">
                    <p class="oe_grey">
                        Are you Sure want to create Permintaan Bahan Baku Harian ?
                    </p>
