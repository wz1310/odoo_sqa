<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_warehouse_view_form" model="ir.ui.view">
        <field name="name">stock.warehouse.view.form</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="mrp.view_warehouse_inherit_mrp"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='pbm_loc_id']" position="after">
            	<field name="location_raw_material_id" string="Location Raw Material 1"/>
                <field name="location_raw_material_id_2" string="Location Raw Material 2"/>
            </xpath>
            <xpath expr="//field[@name='manu_type_id']" position="after">
                <field name="raw_material_type_id"/>
            	<field name="production_type_id"/>
            </xpath>
        </field>
    </record>

</odoo>