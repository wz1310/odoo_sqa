<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mrp_shift_search_view" model="ir.ui.view" >
        <field name="name">mrp.production.schedule.user.search.view</field>
        <field name="model">mrp.production.schedule</field>
        <field name="arch" type="xml">
          <search>
              <field name="product_id"/>
              <field name="warehouse_id"/>
          </search>
        </field>
    </record>

    <record id="action_mrp_mps_user" model="ir.actions.client">
        <field name="name">Master Production Schedule User</field>
        <field name="res_model">mrp.production.schedule</field>
        <field name="tag">mrp_mps_client_action_user</field>
    </record>

    <menuitem
        id="mrp_mps_report_menu_user"
        name="Master Production Schedule User"
        action="action_mrp_mps_user"
        sequence="1"
        parent="mrp_mps.mrp_mps_menu_planning"/>
</odoo>
