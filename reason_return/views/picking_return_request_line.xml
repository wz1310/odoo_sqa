<?xml version="1.0" encoding="utf-8"?>
<odoo> 
    <record model="ir.ui.view" id="picking_return_request_line_tree_view">
        <field name="name">picking.return.request.line.tree.view</field>
        <field name="model">picking.return.request.line</field>
        <field name="arch" type="xml">
            <tree string="Tree Line Return Barang">
                <field name="request_id" invisible="1"/>
                <field name="sale_line_id" />
                <field name="product_id" />
                <field name="lot_id" />
                <field name="delivered_qty" />
                <field name="qty"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="picking_return_request_line_form_view">
        <field name="name">picking.return.request.line.form.view</field>
        <field name="model">picking.return.request.line</field>
        <field name="arch" type="xml">
            <form string="Form Line Return Barang">
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="sale_line_id" />
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="product_id" />
                            <field name="lot_id" />
                        </group>
                        <group>
                            <field name="delivered_qty" />
                            <field name="qty"/>
                        </group>
                    </group>
                </sheet>
                <!--<footer></footer>-->
                
                <!-- UNCOMMENT IF USING Mail Track SYSTEM -->
                <!--<div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>-->
            </form>
        </field>
    </record>
</odoo>