<?xml version="1.0"?>
<odoo>
    <data>

        <record id="paperformat_helpdesk" model="report.paperformat">
            <field name="name">A5 helpdesk report</field>
            <field name="format">A5</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>

    <report id="help_report_uniq_id"
           name="helpdesk_lite.help_report_template_document"
           report_type="qweb-pdf"
           model="helpdesk_lite.ticket"
           file="helpdesk_lite.help_report_template_document"
           string="Print Ticket"
           groups="helpdesk_lite.group_helpdesk_lite_manager"
           paperformat="helpdesk_lite.paperformat_helpdesk"
           />
     </data>
</odoo>
