            </xpath>
        </field>
    </record>

      <record model="ir.ui.view" id="mis_stock_picking_v1_inherit_stock_view_picking_form">
        <field name="name">Stock Picking Form MIS_V1</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='picking_type_id']" position="before">
                <field name="domain_picking_type_id" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='domain_picking_type_id']" position="after">
                <field name="domain_location_id" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='picking_type_id']" position="attributes">
                <attribute name="attrs">{'readonly': [('state', 'in', ('cancel','done'))]}</attribute>
                <attribute name="domain">[('id','=',domain_picking_type_id)]</attribute>
            </xpath>
            <xpath expr="//sheet/group/group/field[@name='location_id']" position="attributes">
                <attribute name="attrs">{'readonly': [('state', 'in', ('cancel','done'))], 'invisible': [('picking_type_code', '=', 'incoming')]}</attribute>
                <attribute name="domain">[('id','=',domain_location_id)]</attribute>
            </xpath>
            <xpath expr="//page[3]" position="inside">
                <group name="desc">
                    <group>
                        <field name="descript" placeholder="Add description..." nolabel="1"
                            attrs="{'readonly': [('state', '=', 'done')]}"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>

