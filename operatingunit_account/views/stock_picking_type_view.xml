<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_type_view_form" model="ir.ui.view">
        <field name="name">stock.picking.type.view.form</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group[2]/group[3]" position="after">
                <group string="Analytic Account">
            	    <field name="analytic_account_id" options = "{'no_create': True}"/>
                </group>
                <group string="Journal">
            	    <field name="journal_id" options = "{'no_create': True}"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>